{
    "name": "pradeep/laracurl",
    "description": "Laravel-cUrl package",
    "license": "MIT",
    "authors": [
        {
            "name": "Pradeep Rajapaksha",
            "email": "pradeepprasanna.rajapaksha4@gmail.com"
        }
    ],
    "require": {
	    "php" : "^7.0",
	    "illuminate/support": "~5.5.0"
	},
	"require-dev": {
	    "phpunit/phpunit": "5.*|^6.3",
	    "orchestra/testbench": "~3.4.0|~3.5.0"
    },
    "autoload": {
        "psr-4": {
            "Pradeep\\LaraCurl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pradeep\\LaraCurl\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pradeep\\LaraCurl\\LaraCurlServiceProvider"
            ]
        }
    }
}
